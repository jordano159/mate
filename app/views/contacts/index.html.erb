<h1>Contacts</h1>

<table class="responsive nowrap table table-hover" id="dttb-contacts" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="name">Name</th>
      <th data-data="email">Email</th>
      <th data-data="message">Message</th>
      <th data-data="tel">Tel</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Contact', new_contact_path %>