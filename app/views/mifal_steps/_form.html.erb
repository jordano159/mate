<%= form_with model: @mifal, url: wizard_path, local: true do |form| %>


  <%= yield form %>

  <div class="form-actions mb-2 mt-2">
    <%= form.submit "המשך", class: "btn btn-primary", id: "submit" %>
    <%= link_to "דילוג לשלב הבא", next_wizard_path, class: "btn btn-secondary" %>
  </div>
<% end %>
