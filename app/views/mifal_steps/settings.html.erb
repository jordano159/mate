<%= render "progress", step: step %>

<div class="m-2">
<h1 class="mb-5">הגדרות מפעל</h1>
<%= render layout: 'form' do |form| %>
  <div class="container mt-2">
    <div class="row">
      <div class="col-lg">
        <div class="field mb-3">
          <%= form.label :has_buses, "צריך ניהול אוטובוסים?" %>
          <div class="container">
            <div class="button-wrap mr-4  marginable" >
              <%= form.collection_radio_buttons :has_buses, [[true, '<span id="buses_true" class="true">כן</span>'.html_safe] ,[false, '<span id="buses_false" class="false">לא</span>'.html_safe]],
              :first, :last, { checked: @mifal.has_buses } %>

            </div>
          </div>
        </div>

        <div class="field mb-3">
          <%= form.label :has_events, "צריך יומן אירועים?" %>
          <div class="container">
            <div class="button-wrap mr-4  marginable">
              <%= form.collection_radio_buttons :has_events, [[true, '<span id="events_true" class="true">כן</span>'.html_safe] ,[false, '<span id="events_false" class="false">לא</span>'.html_safe]],
              :first, :last  %>
            </div>
          </div>
        </div>
        <div class="field mb-3">
          <%= form.label :has_approve, "צריך שהרמה השנייה תאשר נוכחות?" %>
          <div class="container">
            <div class="button-wrap mr-4  marginable">
              <%= form.collection_radio_buttons :has_approve, [[true, '<span id="approve_true" class="true">כן</span>'.html_safe] ,[false, '<span id="approve_false" class="false">לא</span>'.html_safe]],
              :first, :last  %>
            </div>
          </div>
        </div>
      </div>
      <div class="col-lg">
        <div class="field mb-3">
          <%= form.label :has_axes, "צריך רמה שלישית?" %>
          <div class="container">
            <div class="button-wrap mr-4  marginable">
              <%= form.collection_radio_buttons :has_axes, [[true, '<span id="axes_true" class="true">כן</span>'.html_safe],
                                                            [false, '<span id="axes_false" class="false">לא</span>'.html_safe]],
              :first, :last  %>
            </div>
          </div>
        </div>
        <div class="field mb-3">
          <%= form.label :has_late, "צריך אפשרות לסמן איחורים?" %>
          <div class="container">
            <div class="button-wrap mr-4  marginable">
              <%= form.collection_radio_buttons :has_late, [[true, '<span id="late_true" class="true">כן</span>'.html_safe],
                                                            [false, '<span id="late_false" class="false">לא</span>'.html_safe]],
              :first, :last  %>
            </div>
          </div>
        </div>

        <div class="field mb-3">
          <%= form.label :checks_num, "מה תדירות הנוכחות?" %>
          <div class="container">
            <div class="button-wrap mr-4 marginable">
              <%= form.collection_radio_buttons :checks_num, [[true, '<span id="checks_num_true" class="true">פעם ביום</span>'.html_safe],
                                                              [false, '<span id="checks_num_false" class="false">מספר פעמים ביום</span>'.html_safe]],
              :first, :last  %>
            </div>
          </div>
        </div>

        <div class="field mb-3">
          <%= form.label :check_fever, "צריך למדוד חום?" %>
          <div class="container">
            <div class="button-wrap mr-4 marginable">
              <%= form.collection_radio_buttons :check_fever, [[true, '<span id="fever_true" class="true">כן</span>'.html_safe],
                                                              [false, '<span id="fever_false" class="false">לא</span>'.html_safe]],
              :first, :last  %>
            </div>
          </div>
        </div>

      </div>
    </div>
  </div>
<hr>
